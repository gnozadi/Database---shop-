<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">

    <children>
        <Label text="Username" alignment="CENTER" layoutX="88.0" layoutY="80.0" prefHeight="31.0" prefWidth="100.0"/>

        <Label text="Password" alignment="CENTER" layoutX="88.0" layoutY="130.0" prefHeight="31.0" prefWidth="100.0"/>

        <TextField promptText="Enter Username" fx:id="tf_user_field" layoutX="240.0" layoutY="80.0" prefHeight="31.0" prefWidth="220.0"/>

        <PasswordField promptText="Enter Password" fx:id="pf_pass_field" layoutX="240.0" layoutY="130.0" prefHeight="31.0" prefWidth="220.0"/>

        <Label text="Do not have account? sign up" onMouseClicked="#goSignUp" alignment="CENTER" layoutX="80.0" layoutY="200.0" prefHeight="31.0" prefWidth="402.0" textAlignment="CENTER"/>

        <Button text="Login" layoutX="250.0" layoutY="250.0" mnemonicParsing="false" onAction="#logIn" prefHeight="31.0" prefWidth="75.0"/>

        <Label fx:id="lbl_message" text="" alignment="CENTER" layoutX="80.0" layoutY="330.0" prefHeight="31.0" prefWidth="402.0" textAlignment="CENTER"/>

    </children>
</AnchorPane>
